let nums16 = [1, 2, 3, 0, 0, 0];
let m=3

let nums17 = [2, 5, 6];
let j=3



var merge = function (nums1, m, nums2, j) {
    let array = [];
    for (let i = 0; i < m; i++){
            array.push(nums1[i]);

        }
    
    for (let i = 0; i < j; i++){
        array.push(nums2[i])
        
    }
    array1 = array.sort((a, b) => a - b);
    return array1
};
console.log(merge(nums16, m, nums17, j));
